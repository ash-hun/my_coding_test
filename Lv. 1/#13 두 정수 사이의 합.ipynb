    "def solution(a, b):\n",
    "    if a==b:\n",
    "        return a\n",
    "    elif a>b:\n",
    "        listup = [ i for i in range(b, a+1)]\n",
    "        return sum(listup)\n",
    "    else:\n",
    "        listup = [ i for i in range(a, b+1)]\n",
    "        return sum(listup)"
