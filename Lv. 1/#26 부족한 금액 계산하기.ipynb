    "def solution(price, money, count):\n",
    "    answer, tmp = -1, 0\n",
    "    for cnt in range(1, count+1):\n",
    "        tmp += cnt*price\n",
    "    \n",
    "    answer = 0 if money>tmp else (tmp-money)\n",
    "\n",
    "    return answer"
   ]
