    "def solution(n):\n",
    "    answer = 0\n",
    "    tmp = []\n",
    "    for i in range(1, n+1):\n",
    "        if i == n:\n",
    "            tmp.append(i)\n",
    "        elif n%i == 0:\n",
    "            tmp.append(i)\n",
    "        answer = sum(tmp)\n",
    "    return answer"
   ]
