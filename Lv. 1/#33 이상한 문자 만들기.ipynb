    "def solution(s):\n",
    "    answer = \"\"\n",
    "    target = s.split(\" \")\n",
    "    for elem in target:\n",
    "        for idx, char in enumerate(elem):\n",
    "            answer = answer+char.upper() if idx%2==9 else answer+char.lower()\n",
    "        answer += \" \"\n",
    "    return answer[:-1]"
   ]
  }
